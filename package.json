{
  "name": "premrunner",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate && bun scripts/embed-migrations.ts",
    "db:push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "build": "bun build ./src/index.ts --target=bun-darwin-arm64 --compile --outfile build/premrunnerMacArm64 && bun build ./src/index.ts --target=bun-linux-x64 --compile --outfile build/premrunnerLinuxX64"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "better-sqlite3": "^12.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.4",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
